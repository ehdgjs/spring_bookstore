{{>layout/header}}
{{>layout/nav}}
<div class="container">
    <form>
        <div class="form-group">
            <label for="id">아이디</label>
            <input type="text" class="form-control" id="id" placeholder="아이디를 입력해주세요" required>
        </div>
        <div class="form-group">
            <label for="pw">비밀번호</label>
            <input type="password" class="form-control" id="pw" placeholder="비밀번호" required>
        </div>
        <div class="form-group">
            <label for="name">이름</label>
            <input type="text" class="form-control" id="name" placeholder="이름을 입력해주세요" required>
        </div>
    </form>
    <button type="button" id="btn-signup" class="btn btn-secondary">회원가입</button>
</div>
{{>layout/footer}}