{{>layout/nav}}
<form>
    <div class="form-group">
        <label for="InputId">아이디</label>
        <input type="text" class="form-control" id="InputId" placeholder="아이디를 입력해주세요" required>
    </div>
    <div class="form-group">
        <label for="InputPw">비밀번호</label>
        <input type="password" class="form-control" id="InputPw" placeholder="비밀번호" required>
    </div>
    <div class="form-group">
        <label for="InputName">이름</label>
        <input type="text" class="form-control" id="InputName" placeholder="이름을 입력해주세요" required>
    </div>
</form>
<button type="button" id="btn-signup" class="btn btn-primary">회원가입</button>
